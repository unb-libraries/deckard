name: 'libpages'
stack:
    classname: 'RagStack'
collectors:
    -
        name: 'Library Pages'
        classname: 'LibPagesCollector'
        output: 'collector/libpages/data/output'
        config:
            cache_urls: True
            ignore_links: True
            images_to_alt: True
            unicode_snob: True
context:
    size: 4096
    max_vector_distance: 1.0
chunker:
    classname: CharacterTextSplitterChunker
    config:
        chunk_size: 1024
        overlap: 128
        split_on: "\n"
        add_document_metadata: False
        write_chunks_to_disk: True
context_builder:
    classname: 'SimpleContextAggregator'
context_database:
    classname: 'SQLite'
    name: 'libpages'
embedding_database:
    classname: 'LanceDB'
    name: 'libpages'
embedding_encoder:
    classname: 'SentenceTransformerEncoder'
    model: 'avsolatorio/GIST-large-Embedding-v0'
query_processor:
    classname: 'StandardQueryProcessor'
reranker:
    classname: 'SentenceTransformerEncoder'
    model: 'BAAI/bge-reranker-large'
    n_results: 5
