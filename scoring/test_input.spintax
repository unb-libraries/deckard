name: 'libpages'
stack:
    class: 'RagStack'
collectors:
    -
        name: 'Library Pages'
        class: 'LibPagesCollector'
        output: 'collector/libpages/data/output'
context:
    size: 4096
    max_vector_distance: 1.0
chunker:
    class: StandardChunker
    size: {512|1024|2048}
    overlap: 128
context_builder:
    class: 'SimpleContextAggregator'
context_database:
    class: 'SQLite'
    name: 'libpages'
embedding_database:
    class: 'LanceDB'
    name: 'libpages'
embedding_encoder:
    class: 'SentenceTransformerEncoder'
    model: 'avsolatorio/GIST-large-Embedding-v0'
query_processor:
    class: {'StandardQueryProcessor'|'RawQueryProcessor'}
reranker:
    class: 'SentenceTransformerEncoder'
    model: 'BAAI/bge-reranker-large'
    n_results: 5
