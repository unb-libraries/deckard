name: 'libpages'
stack:
    classname: 'RagStack'
collectors:
    -
        name: 'Library Pages'
        classname: 'LibPagesCollector'
        output: 'collector/libpages/data/output'
context:
    size: 4096
    max_vector_distance: 1.0
chunker:
    classname: StandardChunker
    size: {512|1024|2048}
    overlap: 128
context_builder:
    classname: 'SimpleContextAggregator'
context_database:
    classname: 'SQLite'
    name: 'libpages'
embedding_database:
    classname: 'LanceDB'
    name: 'libpages'
embedding_encoder:
    classname: 'SentenceTransformerEncoder'
    model: '{avsolatorio/GIST-large-Embedding-v0|BAAI/bge-large-en-v1.5|hkunlp/instructor-large|sentence-transformers/all-MiniLM-L12-v2}'
query_processor:
    classname: {'StandardQueryProcessor'|'RawQueryProcessor'}
reranker:
    classname: 'SentenceTransformerEncoder'
    model: '{BAAI/bge-reranker-large|colbert-ir/colbertv2.0}'
    n_results: 5
